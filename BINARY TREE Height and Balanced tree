#include<iostream>
#include<cmath>
using namespace std;
class node
{   
 public:
    int data;
	node*left;
	node*right;
	node(int d)
	{
		data=d;
		left=NULL;
		right=NULL;
    }
};
node* buildtree()
{             
	int d;                                     
	cin>>d;                                  
	if(d==-1)
	{
		return NULL;
	}
	node*root=new node(d);
	root->left=buildtree();
	root->right=buildtree();
	return root;
}
class HBpair
{
	public:
		int height;
		bool balance;
};
HBpair isheightbalance(node*root)
{
	HBpair p;
	if(root==NULL)
	{
		p.height =0;
		p.balance=true;
		return p;
    }
   //recursive case
   HBpair left=isheightbalance(root->left);
   HBpair right=isheightbalance(root->right);

   p.height=max(left.height,right.height)+1;
   if(abs(left.height-right.height)<=1 && left.balance and right.balance)
   {
    	p.balance=true;
   }
   else
   {
    	p.balance=false;
   }
    return p;
}
int main()
{
	node*root=buildtree();
	if(isheightbalance(root).balance)
	{
		cout<<"Balanced";
	}
	else
	{
		cout<<"Not Balanced!!"<<endl;
	}
	return 0;
}
/*
i/p:
8 7 -1 -1 6 -1 -1
o/p:
Balanced
*/
